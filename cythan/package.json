{
    "name": "cythan",
    "displayName": "cythan",
    "description": "Cythan programming language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cythan",
            "aliases": ["cythan", "cythan"],
            "extensions": [".ct1",".ct"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cythan",
            "scopeName": "source.cythan",
            "path": "./syntaxes/cythan.tmLanguage.json"
        }]
    }
}